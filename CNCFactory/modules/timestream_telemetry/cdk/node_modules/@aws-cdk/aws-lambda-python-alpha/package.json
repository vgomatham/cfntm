{
  "name": "@aws-cdk/aws-lambda-python-alpha",
  "version": "2.0.0-rc.24",
  "private": false,
  "description": "The CDK Construct Library for AWS Lambda in Python",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "software.amazon.awscdk.services.lambda.python.alpha",
        "maven": {
          "groupId": "software.amazon.awscdk",
          "artifactId": "lambda-python-alpha"
        }
      },
      "dotnet": {
        "namespace": "Amazon.CDK.AWS.Lambda.Python.Alpha",
        "packageId": "Amazon.CDK.AWS.Lambda.Python.Alpha",
        "iconUrl": "https://raw.githubusercontent.com/aws/aws-cdk/master/logo/default-256-dark.png"
      },
      "python": {
        "distName": "aws-cdk.aws-lambda-python-alpha",
        "module": "aws_cdk.aws_lambda_python_alpha",
        "classifiers": [
          "Framework :: AWS CDK",
          "Framework :: AWS CDK :: 1"
        ]
      },
      "go": {
        "moduleName": "github.com/aws/aws-cdk-go"
      }
    },
    "projectReferences": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/aws-cdk.git",
    "directory": "packages/individual-packages/aws-lambda-python"
  },
  "scripts": {
    "build": "cdk-build",
    "watch": "cdk-watch",
    "lint": "cdk-lint",
    "test": "cdk-test",
    "integ": "cdk-integ",
    "pkglint": "pkglint -f",
    "package": "cdk-package",
    "awslint": "cdk-awslint",
    "build+test+package": "yarn build+test && yarn package",
    "build+test": "yarn build && yarn test",
    "compat": "cdk-compat",
    "rosetta:extract": "yarn --silent jsii-rosetta extract",
    "build+extract": "yarn build && yarn rosetta:extract",
    "build+test+extract": "yarn build+test && yarn rosetta:extract"
  },
  "keywords": [
    "aws",
    "cdk",
    "constructs",
    "lambda"
  ],
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com",
    "organization": true
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@aws-cdk/cdk-build-tools": "2.0.0-rc.24",
    "@aws-cdk/cdk-integ-tools": "2.0.0-rc.24",
    "@aws-cdk/pkglint": "2.0.0-rc.24",
    "@types/jest": "^26.0.24",
    "aws-cdk-lib": "2.0.0-rc.24",
    "constructs": "^10.0.0",
    "@aws-cdk/assertions-alpha": "2.0.0-rc.24"
  },
  "dependencies": {},
  "homepage": "https://github.com/aws/aws-cdk",
  "peerDependencies": {
    "aws-cdk-lib": "^2.0.0-rc.24",
    "constructs": "^10.0.0"
  },
  "engines": {
    "node": ">= 10.13.0 <13 || >=13.7.0"
  },
  "stability": "experimental",
  "maturity": "experimental",
  "awscdkio": {
    "announce": false
  },
  "nozem": {
    "ostools": [
      "docker"
    ]
  },
  "publishConfig": {
    "tag": "latest"
  },
  "awslint": {
    "exclude": [
      "*:*"
    ]
  },
  "pkglint": {
    "exclude": [
      "naming/package-matches-directory",
      "assert/assert-dependency"
    ]
  }
}
